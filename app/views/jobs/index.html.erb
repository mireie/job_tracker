<p id="notice"><%= notice %></p>

<h1>Jobs</h1>

<div class="row">
  <div class="col">
    <% if @jobs.any? %>
      
      <%= render :partial => "job_grid", :locals => {:jobs => @jobs} %>
     
    <% else%>
      <p>There are no jobs yet.</p>
    <% end %>
  </div>
</div>

<br>

<%= link_to 'New Job', new_job_path %>
